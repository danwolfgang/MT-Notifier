create table mt_notifier_queue (
  notifier_queue_id integer not null auto_increment primary key,
  notifier_queue_head_content varchar(75),
  notifier_queue_head_from varchar(75) not null,
  notifier_queue_head_to varchar(75) not null,
  notifier_queue_head_subject varchar(255),
  notifier_queue_body text
);
